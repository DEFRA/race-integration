{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\sqlexpress;Database=RACE2DB;MultipleActiveResultSets=True;Trusted_Connection=True;"

  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApplicationSettings": {
    "AllowedHosts": "*",
    "RACE2FrontEndURL": "http://localhost:5001",
    "RACE2WebApiURL": "http://localhost:5003/graphql/",
    "RACE2SecurityProviderURL": "http://localhost:5010"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs\\AppLogs.log",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "rollingInterval": "Day"
        }
      }
    ]
  } 

}
