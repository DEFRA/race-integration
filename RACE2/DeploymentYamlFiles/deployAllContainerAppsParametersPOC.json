{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {      
      "resourcegroup": {
        "value": "Race2ProjectRG"
      },     
      "managedidentity": {
        "value": "Race2ManagedIdentity"
      }, 
      "race2appenvName": {
        "value": "race2containerappenv"
      }, 
      "aspnetCoreEnv": {
        "value": "Production"
      },
      "appConfigURL": {
        "value": "https://race2appconfig.azconfig.io/"
      },     
      "registryName": {
        "value": "race2acr"
      },
      "useExternalIngress": {
        "value": true
      },
      "containerPort": {
        "value": 80
      },
      "minReplicas": {
        "value": 1
      },
      "maxReplicas": {
        "value": 1
      },
      "securityProviderContainerAppName": {
        "value":"race2securityprovider"
      },
      "frontEndWebServerContainerAppName": {
        "value":"race2frontendwebserver"
      },
      "webApiContainerAppName": {
        "value":"race2webapi"
      },
      "webApiExternalContainerAppName": {
        "value":"race2webapiexternal"
      },
      "securityprovidercontainerImage": {
        "value":"race2acr.azurecr.io/race2securityprovider"
      },
      "frontendwebservercontainerImage": {
        "value":"race2acr.azurecr.io/race2frontendwebserver"
      },
      "webapicontainerImage": {
        "value":"race2acr.azurecr.io/race2webapi"
      },
      "webapiexternalcontainerImage": {
        "value":"race2acr.azurecr.io/race2webapiexternal"
      } 
    }
}
