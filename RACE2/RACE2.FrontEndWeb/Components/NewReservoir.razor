@page "/new-reservoir"

<div class="govuk-width-container ">

    <a class="govuk-back-link" href="javascript:history.back()">Back</a>

    <main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">

        <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">

                <div class="govuk-panel govuk-panel-dropout">
                    <h1 class="govuk-panel__title">
                        You cannot continue with this service.
                    </h1>
                </div>

                <p class="govuk-body govuk-!-margin-top-9">The Reservoir Safety Team will contact you shortly.</p>

            </div>
        </div>

    </main>
</div>

@code {
    [Parameter]
    public string ReservoirName { get; set; } = default!;
    [Parameter]
    public string NearestPostcode { get; set; } = default!;
    [Parameter]
    public string NationalGridRef { get; set; } = default!;
}
